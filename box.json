{
  "alias": "terminus.phar",
  "chmod": "0755",
  "directories": ["assets", "bin", "config", "src", "templates"],
  "files": ["README.md","composer.lock","composer.json"],
  "finder": [
    {
      "exclude": [
        "behat",
        "phpunit",
        "php_codesniffer",
        "php-vcr",
        "prophecy",
        "test",
        "tests",
        "Test",
        "Tests",
        "Tester"
      ],
      "in": "vendor",
      "name": "*.php"
    },
    {
      "in": "vendor/consolidation/annotated-command/src",
      "name": "*.php"
    },
    {
      "in": "vendor/symfony",
      "name": "*.php8"
    }
  ],
  "git-commit": "git-commit",
  "git-version": "git-version",
  "output": "terminus.phar",
  "main": "bin/terminus",
  "stub": true
}
