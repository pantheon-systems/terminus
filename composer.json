{
  "name": "pantheon-systems/cli",
  "description": "A command line interface for Pantheon",
  "keywords": [ "cli", "pantheon", "terminus", "drupal", "wordpress" ],
  "homepage": "http://getpantheon.com",
  "license": "MIT",
  "bin": [
    "bin/terminus.bat", "bin/terminus"
  ],
  "require": {
    "php": ">=5.5.0",
    "guzzlehttp/guzzle": "6.1.*@stable",
    "jlogsdon/cli": "~0.9.4",
    "katzgrau/klogger": "1.2.*@stable",
    "mustache/mustache": "2.4.*",
    "nb/oxymel": "0.1.0",
    "psy/psysh": "@stable",
    "ramsey/array_column": "1.1.*",
    "rmccue/requests": "1.6.*",
    "symfony/finder": "2.7.*",
    "symfony/process": "2.7.*",
    "symfony/dom-crawler": "2.7.*",
    "symfony/css-selector": "2.7.*",
    "vlucas/phpdotenv": "2.1.*@stable"
  },
  "autoload": {
    "psr-0": { "Terminus": "php" }
  },
  "require-dev": {
    "behat/behat": "3.0.*@stable",
    "phpunit/phpunit": "5.0.*",
    "php-vcr/php-vcr": "1.2.*",
    "php-vcr/phpunit-testlistener-vcr": "*",
    "sebastian/phpcpd": "*",
    "squizlabs/php_codesniffer": "2.*"
  }
}
